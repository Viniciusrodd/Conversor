<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Table</title>
</head>
<body>
    <table>
        <thead>
            <% header.forEach((col) =>{ %>
                <th><%= col %></th>                
            <% }) %> 
        </thead>
        <tbody>
            <% body.forEach((row) =>{ %>
                <tr>
                    <% row.forEach((elements) =>{ %>
                        <td> <%= elements %> </td>
                    <% }) %>
                </tr>                
            <% }) %> 
        </tbody>
    </table>
</body>
</html>